{
	"name": "akeeba-panopticon",
	"description": "Akeeba Panopticon",
	"license": "AGPL-3.0-or-later",
	"repository": {
		"type": "git",
		"url": "https://github.com/akeeba/panopticon.git"
	},
	"engines": {
		"node": ">=16",
		"npm": ">=8.5.5"
	},
	"browserslist": [
		"last 2 major version",
		"not ie < 11"
	],
	"devDependencies": {
		"@babel/cli": "^7.23.0",
		"@babel/core": "^7.22.19",
		"@babel/preset-env": "^7.11.5",
		"@fortawesome/fontawesome-free": "^6.4.0",
		"ace-builds": "^1.25.1",
		"babel-preset-minify": "^0.5.1",
		"bootstrap": "^5.3.2",
		"choices.js": "^10.2.0",
		"core-js": "^3.9.1",
		"tinymce": "^6.4.2"
	},
	"babel": {
		"presets": [
			[
				"@babel/preset-env",
				{
					"useBuiltIns": "entry",
					"corejs": {
						"version": "3.9",
						"proposals": true
					}
				}
			],
			[
				"minify",
				{
					"builtIns": false,
					"removeConsole": false
				}
			]
		],
		"comments": false,
		"sourceType": "script",
		"ignore": [
			"**/bootstrap.bundle.*",
			"**/*.min.js",
			"**/*.min.css"
		]
	}
}
