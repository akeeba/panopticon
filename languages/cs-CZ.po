msgctxt "PANOPTICON_LOGS_LBL_NOVIEW_HELP"
msgid "You cannot view an archived log file."
msgstr "Nelze zobrazit archivovaný protokolový soubor."

msgctxt "PANOPTICON_LOGS_LBL_ARCHIVED_OPT_ALL"
msgid "All"
msgstr "Vše"

msgctxt "PANOPTICON_LOGS_LBL_ARCHIVED_OPT_ACTIVE"
msgid "Only Active"
msgstr "Pouze aktivní"

msgctxt "PANOPTICON_LOGS_ERR_NOTHING_TO_SEE"
msgid "Nothing to see here."
msgstr "Není co zobrazit."

msgctxt "PANOPTICON_LOGS_ERR_NOTHING_TO_SEE_DETAILS"
msgid ""
