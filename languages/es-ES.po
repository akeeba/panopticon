msgstr "Continuará ejecutando eventos posteriores a la actualización para el sitio %d (%s)"

msgctxt "PANOPTICON_TASK_JOOMLAUPDATE_LOG_POSTUPDATE_FINISHED"
msgid "Finished executing post-update events for site %d (%s)"
msgstr "Finalizada la ejecución de eventos posteriores a la actualización para el sitio %d (%s)"

msgctxt "PANOPTICON_TASK_JOOMLAUPDATE_LOG_RELOAD_UPDATES"
