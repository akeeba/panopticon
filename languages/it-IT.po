msgstr "Devi inserire il Token API."

msgctxt "PANOPTICON_SITES_ERR_CONNECTION_ERROR"
msgid "Could not connect to the remote site."
msgstr "Non è stato possibile connettersi al sito remoto."

msgctxt "PANOPTICON_SITES_TABLE_COMMENT"
msgid "Table of sites known to Panopticon"
msgstr "Tabella dei siti conosciuti da Panopticon"

msgctxt "PANOPTICON_SITES_TABLE_HEAD_NAME"
msgid "Site"
msgstr "Sito"

msgctxt "PANOPTICON_SITE_LBL_TAB_CONNECTION"
msgid "Connection"
msgstr "Connessione"

msgctxt "PANOPTICON_SITE_LBL_TAB_PROPERTIES"
