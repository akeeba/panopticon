msgid "Joomla!&reg; Update"
msgstr "Aggiornamento Joomla!&reg;"

msgctxt "PANOPTICON_SITE_LBL_TAB_EXTUPDATE"
msgid "Extensions Update"
msgstr "Aggiornamento estensioni"

